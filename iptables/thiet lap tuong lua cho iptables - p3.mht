/**
* Attempts to display a post via AJAX, falling back to opening a new window=
 if AJAX not available
*
* @param	integer	Post ID
*
* @return	boolean	False
*/
function display_post(postid)
{
	if (AJAX_Compatible)
	{
		vB_PostLoader[postid] =3D new vB_AJAX_PostLoader(postid);
		vB_PostLoader[postid].init();
	}
	else
	{
		pc_obj =3D fetch_object('postcount' + this.postid);
		openWindow('showpost.php?' + (SESSIONURL ? 's=3D' + SESSIONURL : '') + (p=
c_obj !=3D null ? '&postcount=3D' + PHP.urlencode(pc_obj.name) : '') + '&p=
=3D' + postid);
	}
	return false;
};

